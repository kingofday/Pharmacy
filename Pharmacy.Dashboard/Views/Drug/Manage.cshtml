@model PagingListDetails<Drug>
@{
    ViewBag.Title = $"{Strings.Management} {DomainStrings.Drug}";
    Layout = "_LayoutManage";
}
@section styles{
    <link href="@Url.Content("~/Styles/App/01-Admin/Product/manage.css")" rel="stylesheet" />
    <link href="~/Styles/Libs/hummingbird.css" rel="stylesheet" />
}

@section filter{
    <partial name="Partials/_Filters" model="new DrugSearchFilter()" />
}
<partial name="Partials/_List" model="@Model" />
@section scripts{
    <script src="~/Scripts/Libs/hummingbird.js"></script>
    <script src="@Url.Content("~/Scripts/App/01-Admin/Product/manage.js")"></script>
}